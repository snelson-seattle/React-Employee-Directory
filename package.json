{
  "name": "react-employee-directory",
  "version": "1.0.0",
  "description": "Employee Directory using React.js",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "adminSeed": "node scripts/adminSeed.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snelson-seattle/React-Employee-Directory.git"
  },
  "author": "Scott Nelson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/snelson-seattle/React-Employee-Directory/issues"
  },
  "homepage": "https://github.com/snelson-seattle/React-Employee-Directory#readme",
  "dependencies": {
    "express": "^4.17.1"
  }
}
